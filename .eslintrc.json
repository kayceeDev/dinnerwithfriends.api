{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "no-param-reassign": 0,
      "prefer-destructuring": 0,
      "max-len": 0,
      "no-underscore-dangle": 0,
      "consistent-return": 0,
      "func-names": 0
    }
}